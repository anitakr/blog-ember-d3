From project root:
git checkout gh-pages
git reset --hard master
nvm use
ember build --environment=production
rm .editorconfig .ember-cli .eslintignore .eslintrc.js .nvmrc .template-lintrc.js .travis.yml .watchmanconfig README.md
rm -rf app config ember-cli-build.js package-lock.json package.json public testem.js tests vendor
mv dist/* .
rmdir dist
git add .
git commit -m 'Create a GH pages view'
git status
git pu -f

